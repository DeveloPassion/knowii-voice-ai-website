{
	"name": "knowii-voice-ai-website",
	"private": true,
	"version": "0.0.1",
	"description": "Marketing website for Knowii Voice AI",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"prepare": "husky",
		"lint": "eslint . --report-unused-disable-directives --max-warnings 0",
		"format": "prettier --write .",
		"tsc": "tsc --noEmit",
		"tsc:watch": "tsc --noEmit --watch --preserveWatchOutput",
		"tscw": "npm run tsc:watch",
		"commit": "cz",
		"cm": "npm run commit",
		"commit:lint": "commitlint --from HEAD~1 --to HEAD --verbose",
		"release": "release-it",
		"release:final": "npm run release",
		"release:alpha": "release-it --preRelease=alpha",
		"release:beta": "release-it --preRelease=beta",
		"release:rc": "release-it --preRelease=rc"
	},
	"lint-staged": {
		"**/*": "prettier --write --ignore-unknown"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-customizable"
		},
		"cz-customizable": {
			"config": ".cz-config.cjs"
		}
	},
	"dependencies": {
		"@tailwindcss/vite": "4.1.16",
		"clsx": "^2.1.1",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-icons": "5.5.0",
		"react-router": "^7.9.3",
		"tailwind-merge": "^3.3.1"
	},
	"devDependencies": {
		"@commitlint/cli": "20.1.0",
		"@commitlint/config-conventional": "20.0.0",
		"@eslint/js": "^9.36.0",
		"@release-it/conventional-changelog": "10.0.1",
		"@types/node": "24.9.1",
		"@types/react": "^19.1.16",
		"@types/react-dom": "^19.1.9",
		"@vitejs/plugin-react": "5.1.0",
		"auto-changelog": "2.5.0",
		"commitizen": "4.3.1",
		"cz-customizable": "7.2.1",
		"eslint": "^9.36.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-react": "^7.37.5",
		"globals": "^16.4.0",
		"husky": "^9.1.7",
		"lint-staged": "16.2.6",
		"prettier": "3.6.2",
		"release-it": "19.0.5",
		"tailwindcss": "4.1.16",
		"typescript": "^5.9.3",
		"typescript-eslint": "8.46.2",
		"vite": "6.4.1"
	}
}
